{
  "name": "aegicloud-server",
  "version": "0.1.0",
  "description": "Backend for AegiCloud. ",
  "author": "Il Harper",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server --exec babel-node -e js",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore . --fix"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@nuxtjs/axios": "^5.3.6",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "js-base64": "^2.5.1",
    "koa": "^2.6.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "node-machine-id": "^1.1.12",
    "node-schedule": "^1.3.2",
    "nuxt": "^2.0.0",
    "uuid": "^3.3.2",
    "vue-i18n": "^8.14.0",
    "vuetify": "^2.0.5",
    "ws": "^7.1.1"
  },
  "devDependencies": {
    "@mdi/font": "^3.9.97",
    "@mdi/js": "^3.9.97",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.2.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.6.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.16.4",
    "sass-loader": "^7.2.0"
  }
}
